<StructureDefinition xmlns="http://hl7.org/fhir">
   <id value="ch-emed-medication-medicationdispense"/>
   <url value="http://fhir.ch/ig/ch-emed/StructureDefinition/ch-emed-medication-medicationdispense"/>
   <version value="0.1.0"/>
   <name value="CHEMEDMedicationMedicationDispense"/>
   <title value="CH EMED Medication Profile (MedicationDispense)"/>
   <status value="draft"/>
   <experimental value="false"/>
   <publisher value="eHealth Suisse"/>
   <description value="Definition of the medication for the medication dispense"/>
   <mapping>
      <identity value="cda"/>
      <uri value="http://hl7.org/v3/cda"/>
      <name value="CDA (R2)"/>
   </mapping>
   <kind value="resource"/>
   <abstract value="false"/>
   <type value="Medication"/>
   <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Medication"/>
   <derivation value="constraint"/>
   <differential>
      <element id="Medication">
         <path value="Medication"/>
         <short value="CH EMED Medication (MedicationDispense)"/>
         <mapping>
            <identity value="cda"/>
            <map value=".consumable.manufacturedProduct.manufacturedMaterial"/>
         </mapping>
      </element>
      <element id="Medication.code">
         <path value="Medication.code"/>
         <min value="1"/>
      </element>
      <element id="Medication.code.coding">
         <path value="Medication.code.coding"/>
         <short value="GTIN code that identify this medication"/>
         <mapping>
            <identity value="cda"/>
            <map value=".code"/>
         </mapping>
         <mapping>
            <identity value="cda"/>
            <map value=".pharm:asContent.pharm:containerPackagedMedicine.pharm:code"/>
         </mapping>
      </element>
      <element id="Medication.code.coding.system">
         <path value="Medication.code.coding.system"/>
         <short value="Global Trade Item Number (GS1)"/>
         <min value="1"/>
         <max value="1"/>
         <type>
            <code value="uri"/>
         </type>
         <fixedUri value="urn:oid:2.51.1.1"/>
      </element>
      <element id="Medication.code.coding.code">
         <path value="Medication.code.coding.code"/>
         <min value="1"/>
         <max value="1"/>
      </element>
      <element id="Medication.code.coding.display">
         <path value="Medication.code.coding.display"/>
         <min value="1"/>
         <max value="1"/>
      </element>
      <element id="Medication.code.text">
         <path value="Medication.code.text"/>
         <short value="Name that identify this medication"/>
         <min value="1"/>
         <mapping>
            <identity value="cda"/>
            <map value=".name"/>
         </mapping>
         <mapping>
            <identity value="cda"/>
            <map value=".pharm:asContent.pharm:containerPackagedMedicine.pharm:name"/>
         </mapping>
      </element>
      <element id="Medication.form">
         <path value="Medication.form"/>
         <short value="Galenic form"/>
         <min value="1"/>
         <binding>
            <strength value="required"/>
            <valueSet value="http://fhir.ch/ig/ch-emed/ValueSet/edqm-pharmaceuticaldoseform"/>
         </binding>
      </element>
      <element id="Medication.form.coding">
         <path value="Medication.form.coding"/>
         <min value="1"/>
         <mapping>
            <identity value="cda"/>
            <map value=".pharm:formCode"/>
         </mapping>
         <mapping>
            <identity value="cda"/>
            <map value=".pharm:asContent.pharm:containerPackagedMedicine.pharm:formCode"/>
         </mapping>
      </element>
      <element id="Medication.form.coding.system">
         <path value="Medication.form.coding.system"/>
         <min value="1"/>
      </element>
      <element id="Medication.form.coding.code">
         <path value="Medication.form.coding.code"/>
         <min value="1"/>
      </element>
      <element id="Medication.form.coding.display">
         <path value="Medication.form.coding.display"/>
         <min value="1"/>
      </element>
      <element id="Medication.amount">
         <path value="Medication.amount"/>
         <short value="Package size"/>
         <min value="1"/>
         <mapping>
            <identity value="cda"/>
            <map value=".pharm:asContent.pharm:containerPackagedMedicine.pharm:capacityQuantity"/>
         </mapping>
      </element>
      <element id="Medication.ingredient">
         <path value="Medication.ingredient"/>
         <min value="1"/>
         <max value="*"/>         
         <mapping>
            <identity value="cda"/>
            <map value=".pharm:ingredient"/>
         </mapping>
      </element>
      <element id="Medication.ingredient.itemCodeableConcept">
         <path value="Medication.ingredient.itemCodeableConcept"/>
         <short value="Name of active ingredient"/>
      </element>
      <element id="Medication.ingredient.itemCodeableConcept.coding">
         <path value="Medication.ingredient.itemCodeableConcept.coding"/>
         <mapping>
            <identity value="cda"/>
            <map value=".pharm:ingredient.pharm:code"/>
         </mapping>
      </element>
      <element id="Medication.ingredient.itemCodeableConcept.coding.system">
         <path value="Medication.ingredient.itemCodeableConcept.coding.system"/>
         <min value="1"/>
         <max value="1"/>
         <type>
            <code value="uri"/>
         </type>
         <fixedUri value="http://snomed.info/sct"/>
      </element>
      <element id="Medication.ingredient.itemCodeableConcept.coding.code">
         <path value="Medication.ingredient.itemCodeableConcept.coding.code"/>
         <min value="1"/>
         <max value="1"/>
      </element>
      <element id="Medication.ingredient.itemCodeableConcept.coding.display">
         <path value="Medication.ingredient.itemCodeableConcept.coding.display"/>
         <min value="1"/>
         <max value="1"/>
      </element>
      <element id="Medication.ingredient.itemCodeableConcept.text">
         <path value="Medication.ingredient.itemCodeableConcept.text"/>
         <mapping>
            <identity value="cda"/>
            <map value=".pharm:ingredient.pharm:name"/>
         </mapping>
      </element>
      <element id="Medication.ingredient.strength">
         <path value="Medication.ingredient.strength"/>
         <short value="Dose per unit"/>
         <min value="1"/>
         <mapping>
            <identity value="cda"/>
            <map value=".pharm:quantity"/>
         </mapping>
      </element>
      <element id="Medication.batch">
         <path value="Medication.batch"/>
         <short value="Details about packaged medications"/>
      </element>
      <element id="Medication.batch.lotNumber">
         <path value="Medication.batch.lotNumber"/>
         <short value="Identifier assigned to batch"/>
         <mapping>
            <identity value="cda"/>
            <map value=".pharm:formCode.lotNumberText"/>
         </mapping>
      </element>
   </differential>
</StructureDefinition>